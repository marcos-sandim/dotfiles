!
!URxvt*font:             xft:Profont:size=8
!URxvt.font:                     xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:size=10
!URxvt.boldFont:                 xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:bold:size=10
!URxvt.italicFont:               xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:italic:size=10
!URxvt.boldItalicFont:           xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:bold:italic:size=10

URxvt.font:                     xft:mononoki\ Nerd\ Font\ Mono:autohint=true:size=16
URxvt.boldFont:                 xft:mononoki\ Nerd\ Font\ Mono:autohint=true:bold:size=16
URxvt.italicFont:               xft:mononoki\ Nerd\ Font\ Mono:autohint=true:italic:size=16
URxvt.boldItalicFont:           xft:mononoki\ Nerd\ Font\ Mono:autohint=true:bold:italic:size=16

URxvt*saveLines:        32767
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*loginShell:       true
URxvt*shading:          30
!URxvt*termName:        rxvt-unicode
URxvt*secondaryScroll:  true
URxvt*perl-lib:         /usr/lib64/urxvt/perl/
! URxvt*perl-ext-common:  default,matcher,searchable-scrollback,clipboard,selection-to-clipboard
URxvt.perl-ext-common:  default,matcher,resize-font
URxvt.url-launcher:     /usr/bin/firefox
URxvt.matcher.button:   1
URxvt*visualBell:       false
!URxvt*keysym.C-u:       perl:matcher
URxvt*urgentOnBell:     true
!URxvt*cursorColor: #b092a4
!URxvt*cursorColor:      #6dacd8
!URxvt*inheritPixmap:    true
!URxvt*tintColor:        #EEEEEE
!URxvt*background:       #000000
!URxvt*foreground:       #e1e2db
URxvt*cursorBlink:       true
!URxvt.keysym.Shift-Control-V: eval:paste_clipboard
!URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

xterm*dynamicColors: true
urxvt*dynamicColors: on

