Xcursor.theme: neutral

!-------------------------------------------------------------------------------
!
!
!      U R X V T
!
!
!URxvt*font:             xft:Profont:size=8
URxvt.font:                     xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:size=10
URxvt.boldFont:                 xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:bold:size=10
URxvt.italicFont:               xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:italic:size=10
URxvt.boldItalicFont:           xft:DejaVu\ Sans\ Mono\ for\ Powerline:autohint=true:bold:italic:size=10

URxvt*saveLines:        32767
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*loginShell:       true
URxvt*shading:          30
!URxvt*termName:         rxvt-unicode
URxvt*secondaryScroll:  true
URxvt*perl-lib:         /usr/lib/URxvt/perl/
URxvt*perl-ext-common:  default,matcher,searchable-scrollback,clipboard,selection-to-clipboard
URxvt*urlLauncher:      chromium-browser
URxvt*matcher.button:   1
!URxvt*cutchars:         `"()'*<>[]{|}
URxvt*visualBell:       false
!URxvt*keysym.C-u:       perl:matcher
URxvt*urgentOnBell:     true
!URxvt*cursorColor: #b092a4
!URxvt*cursorColor:      #6dacd8
!URxvt*inheritPixmap:    true
!URxvt*tintColor:        #EEEEEE
!URxvt*background:       #000000
!URxvt*foreground:       #e1e2db
URxvt*cursorBlink:       true

! special
*.foreground:   #f8f8f2
*.background:   #272822
*.cursorColor:  #f8f8f2

! black
*.color0:       #272822
*.color8:       #75715e

! red
*.color1:       #f92672
*.color9:       #f92672

! green
*.color2:       #a6e22e
*.color10:      #a6e22e

! yellow
*.color3:       #f4bf75
*.color11:      #f4bf75

! blue
*.color4:       #66d9ef
*.color12:      #66d9ef

! magenta
*.color5:       #ae81ff
*.color13:      #ae81ff

! cyan
*.color6:       #a1efe4
*.color14:      #a1efe4

! white
*.color7:       #f8f8f2
*.color15:      #f9f8f5


!-------------------------------------------------------------------------------
!
!
!      X P D F
!
!
xpdf*enableFreetype: yes
xpdf*antialias:  yes
xpdf*foreground: black
xpdf*background: white
xpdf*urlCommand: /usr/bin/firefox %s
xpdf*viKeys:     on

!-------------------------------------------------------------------------------
